{
  "name": "air-quality-private-beta",
  "version": "0.2.0",
  "private": true,
  "main": "",
  "author": {
    "name": "Ulysses Alvarez",
    "url": "https://onecognizant.cognizant.com/"
  },
  "engines": {
    "node": "^20.10.0",
    "npm": "^10.2.3"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run prod",
    "build:sass": "sass --quiet-deps --load-path=node_modules/govuk-frontend --load-path=./assets/main.scss --load-path=./assets/application.scss dist/public/stylesheets/main.css --style=compressed --no-source-map",
    "build": "npm-run-all clean build:sass transpile",
    "server": "node --inspect -r ts-node/register/transpile-only -r tsconfig-paths/register ./server/app.ts",
    "dev": "SET NODE_ENV=development & npm-run-all build server",
    "prod": "SET NODE_ENV=production & npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@govuk-prototype-kit/common-templates": "^2.0.0",
    "@govuk-prototype-kit/step-by-step": "^2.2.2",
    "@govuk-prototype-kit/task-list": "^2.0.0",
    "@types/mssql": "^9.1.4",
    "@x-govuk/govuk-prototype-components": "^3.0.1",
    "@x-govuk/govuk-prototype-filters": "^1.3.0",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "govuk-frontend": "^5.0.0",
    "govuk-prototype-kit": "^13.16.0",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "mssql": "^10.0.2",
    "mysql2": "^3.7.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "rimraf": "^3.0.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "express-session": "^1.17.3",
    "jest": "^25.1.0",
    "sass": "^1.69.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
